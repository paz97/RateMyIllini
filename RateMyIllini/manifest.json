{
  "manifest_version": 2,
  "name": "Rate My Illini",
  "version":"1.5",
  "description":"Pulls UIUC Professors' ratings from ratemyprofessors.com",
  "icons":  {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action":{
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.ratemyprofessors.com/"
  ]

}
